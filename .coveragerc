[run]
source = pingpong
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */virtualenv/*
    */env/*
    */site-packages/*
    manage.py
    */asgi.py
    */wsgi.py
    */settings/*
    */settings.py
    */settings_*.py
    */apps.py
    */admin.py
    ttstats/settings/__init__.py
    ttstats/settings/base.py
    ttstats/settings/dev.py
    ttstats/settings/prod.py
    ttstats/settings_test.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
